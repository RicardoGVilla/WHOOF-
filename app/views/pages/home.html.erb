<div class = 'container py-3'>
  <div class="cards">
    <% @dogs.each do |doggo|  %>
      <div class="row row-cols-3 row-cols-md-2 g-4">
        <div class="col">
          <div class="card">
            <img src="...img goes here" class="card-img-top" alt="...">
            <div class="card-body">
                <p class="card-text"> <%= "price #{doggo.id}" %></p>
                <p class="card-text"></p>
              <p class="card-text">jeff:</p>
              <p class="card-text">location:</p>
            </div>
          </div>
            <%if user_signed_in? %>
              <%= link_to 'Book this dog',new_dog_booking_path(:dog_id), class: 'btn btn-outline-success' %>
            <% end %>
        </div>
      </div>
    <% end %>
  </div>
</div>
